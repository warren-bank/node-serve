{
  "cleanUrls":    false,
  "renderSingle": false,
  "etag":         true,
  "symlinks":     true,
  "logReq":       true,
  "logRes":       true,

  "rewrites": [
    { "source": "/IGNORE_IMPLICIT/:bar*", "destination": "/:bar" }
  ],
  "redirects": [
    { "source": "/IGNORE_EXPLICIT/:bar*", "destination": "/:bar", "preserveQuery": true, "preserveHash": true }
  ],

  "headers": [{
    "source": "**",
    "headers": [
      {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }, {
        "key": "Access-Control-Allow-Headers",
        "value": "Origin, X-Requested-With, Content-Type, Accept, Range"
      }
    ]
  }]
}
