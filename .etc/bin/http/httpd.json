{
  "renderSingle":  false,
  "cleanUrls":     false,
  "trailingSlash": true,
  "etag":          true,
  "symlinks":      true,
  "logReq":        true,
  "logRes":        true,

  "proxyCookieJar": "proxy-cookie-jar/cookies.json",

  "headers": [
    {
      "source":  "**/cgi-bin/**/*.+(pl|sh|bat)",
      "headers": [
        {
          "key":   "Content-Type",
          "value": "text/plain"
        }
      ]
    },
    {
      "source":  "**/cgi-bin/**/*.php",
      "headers": [
        {
          "key":   "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ],

  "rewrites": [
    {
      "engine":        "regex",
      "source":        "^(.*?)(?:/IGNORE_IMPLICIT)+(.*)$",
      "destination":   "$1$2",
      "flags":         "",
      "terminal":      true
    },

    {
      "engine":        "route",
      "source":        ":parent*/(IGNORE_IMPLICIT)+/:pathname*",
      "destination":   ":parent?/:pathname?",
      "terminal":      true
    },

    {
      "engine":        "text",
      "source":        "/IGNORE_IMPLICIT/",
      "destination":   "/",
      "exact":         false,
      "terminal":      true
    },

    {
      "engine":        "text",
      "source":        "/favicon.ico",
      "destination":   "data:image/x-icon;base64,",
      "exact":         true,
      "terminal":      true
    }
  ],

  "redirects": [
    {
      "engine":        "regex",
      "source":        "^(.*?)(?:/(?:IGNORE_IMPLICIT|IGNORE_EXPLICIT))*(/IGNORE_EXPLICIT)(?:/(?:IGNORE_IMPLICIT|IGNORE_EXPLICIT))*((?:/[^/]*)*)(/)(index\\.(?:html|php))$",
      "destination":   "$1$2$3$4?query=$5",
      "flags":         "",
      "decode":        false,
      "terminal":      false,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         false
    },
    {
      "engine":        "regex",
      "source":        "^(.*?)(?:/(?:IGNORE_IMPLICIT|IGNORE_EXPLICIT))*(?:/IGNORE_EXPLICIT)(?:/(?:IGNORE_IMPLICIT|IGNORE_EXPLICIT))*(.*)$",
      "destination":   "$1$2",
      "flags":         "",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": true,
      "preserveHash":  true,
      "proxy":         false
    },

    {
      "engine":        "route",
      "source":        ":parent*/(IGNORE_IMPLICIT)*/(IGNORE_EXPLICIT)+/:dirname*/:basename(index\\.(?:html|php))",
      "destination":   ":parent?/IGNORE_EXPLICIT/:dirname?/\\?query=:basename",
      "decode":        false,
      "terminal":      false,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         false
    },
    {
      "engine":        "route",
      "source":        ":parent*/(IGNORE_IMPLICIT)*/(IGNORE_EXPLICIT)+/:pathname*",
      "destination":   ":parent?/:pathname?",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": true,
      "preserveHash":  true,
      "proxy":         false
    },

    {
      "engine":        "glob",
      "source":        "/**/.gitkeep",
      "destination":   "/test.html",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         false
    },

    {
      "engine":        "text",
      "source":        ".gitignore",
      "destination":   ".gitkeep",
      "exact":         false,
      "decode":        false,
      "terminal":      false,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         false
    },

    {
      "engine":        "regex",
      "source":        "^/echo(/.*)?$",
      "destination":   "https://httpbin.org/anything$1",
      "flags":         "i",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": true,
      "preserveHash":  false,
      "proxy":         false
    },

    {
      "engine":        "regex",
      "source":        "^/search/([^/]+)/?$",
      "destination":   "https://www.google.com/search?q=$1",
      "flags":         "",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         true
    },
    {
      "engine":        "regex",
      "source":        "^/whoami/?$",
      "destination":   "https://httpbin.org/ip",
      "flags":         "i",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         true
    },
    {
      "engine":        "regex",
      "source":        "^/post/?$",
      "destination":   "https://httpbin.org/post",
      "flags":         "i",
      "decode":        false,
      "terminal":      true,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         true
    },

    {
      "engine":        "text",
      "source":        "/favicon.ico",
      "destination":   "data:image/x-icon;base64,",
      "exact":         true,
      "decode":        false,
      "terminal":      true,
      "preserveQuery": false,
      "preserveHash":  false,
      "proxy":         false
    }
  ],

  "proxyMiddleware": [
    {
      "engine":        "text",
      "source":        "https://www.google.com/search?q=",
      "type":          "html",
      "middleware":    "function($) { const results = $('#search'); $('body').empty().append(results); $('div[jscontroller], h2, script, style').remove(); }",
      "terminal":      true
    },
    {
      "engine":        "text",
      "source":        "https://httpbin.org/ip",
      "type":          "json",
      "middleware":    "function(data) { if (data.response instanceof Object) Object.assign(data.response, {hello: 'world'}); }",
      "terminal":      true
    }
  ],

  "cgiBin": [
    {
      "engine":        "glob",
      "source":        "**/cgi-bin/**/*.pl",
      "command":       "perl {{source}}",
      "env":           { "PATH": "C:/PortableApps/perl/5.10.1" }
    },
    {
      "engine":        "glob",
      "source":        "**/cgi-bin/**/*.php",
      "command":       "php {{source}}",
      "env":           { "PATH": "C:/PortableApps/php/8.0.0" }
    },
    {
      "engine":        "glob",
      "source":        "**/cgi-bin/**/*.sh",
      "command":       "bash --noprofile --norc --noediting {{source}}",
      "env":           { "PATH": "C:/PortableApps/PortableGit/2.16.2/bin" }
    },
    {
      "engine":        "glob",
      "source":        "**/cgi-bin/**/*.bat",
      "command":       "cmd.exe /c {{source}}"
    }
  ]
}
